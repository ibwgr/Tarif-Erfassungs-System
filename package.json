{
  "name": "tarif-erfassungs-system",
  "version": "1.0.0",
  "description": "TES Tarif Erfassungs System",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "http-server src/app --port 1234",
    "start:ci": "npm run dist && ./node_modules/.bin/http-server dist --port 1234",
    "start-parcel": "rm -rf .cache && npx parcel src/app/index.html",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "mocha src/test/unit/**.js",
    "test:e2e": "mocha src/test/e2e/**.js",
    "dist": "NODE_ENV=production rm -rf dist && ./node_modules/.bin/parcel build src/app/index.html"
  },
